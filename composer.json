{
    "name": "slince/spike",
    "description": "A fast reverse proxy that helps to expose your local services to the internet",
    "keywords": ["tunnel", "proxy", "reverse proxy", "nat", "expose localhost", "reactphp", "http tunnel", "tcp tunnel"],
    "authors": [
        {
          "name": "Tao",
          "email": "taosikai@yeah.net"
        }
    ],
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7.2||^8.0",
        "ext-json": "*",
        "ext-pcntl": "*",
        "ext-posix": "*",
        "ext-parallel": "*",
        "react/socket": "^1.9",
        "react/promise": "^2.8",
        "react/stream": "^1.2",
        "guzzlehttp/psr7": "^1.8",
        "slince/config": "^1.0",
        "symfony/console": "^3.0|^4.0|^5.0",
        "monolog/monolog": "^2.0",
        "doctrine/collections": "^1.6",
        "symfony/options-resolver": "^5.2",
        "symfony/serializer": "^5.2",
        "react/child-process": "^0.6",
        "symfony/process": "^5.3",
        "walkor/workerman": "^4.0",
        "symfony/yaml": "^5.3",
        "symfony/property-access": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Spike\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spike\\Tests\\": "./tests"
        }
    },
    "bin": ["bin/spike", "bin/spiked"],
    "minimum-stability": "stable"
}

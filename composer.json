{
    "name": "slince/spike",
    "description": "A fast reverse proxy that helps to expose your local services to the internet",
    "keywords": ["tunnel", "proxy", "reverse proxy", "nat", "expose localhost", "reactphp", "http tunnel", "tcp tunnel"],
    "authors": [
        {
          "name": "Tao",
          "email": "taosikai@yeah.net"
        }
    ],
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7.2||^8.0",
        "react/socket": "^1.6",
        "react/promise": "^2.8",
        "react/stream": "^1.1",
        "guzzlehttp/psr7": "^1.8",
        "slince/event-dispatcher": "^2.0",
        "slince/config": "^1.0",
        "symfony/console": "^3.0|^4.0|^5.0",
        "monolog/monolog": "^2.0",
        "doctrine/collections": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Spike\\": "./src"
        },
        "files": ["./src/Common/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Spike\\Tests\\": "./tests"
        }
    },
    "bin": ["bin/spike", "bin/spiked"],
    "minimum-stability": "stable"
}
